<?xml version="1.0"?>

<PropertyList>

<path>eng-fuel.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>amp.needle</object-name>
        <property>systems/electrical/amps</property>
        <interpolation>
            <entry><ind>-60.0</ind><dep>-28.0</dep></entry>
            <entry><ind>60.0</ind><dep>28.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.019</y-m>
            <z-m>0.031</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>oilP.needle</object-name>
