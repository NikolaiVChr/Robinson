<?xml version="1.0"?>

<PropertyList>

<path>vsi.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>VSI.needle</object-name>
        <property>/instrumentation/vertical-speed-indicator/indicated-speed-fpm</property>
        <interpolation>
            <entry><ind>-2000.0</ind><dep>-170.0</dep></entry>
            <entry><ind>-1000.0</ind><dep>-85.0</dep></entry>
            <entry><ind>-500.0</ind><dep>-42.5</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>500.0</ind><dep>42.5</dep></entry>
            <entry><ind>1000.0</ind><dep>85.0</dep></entry>
            <entry><ind>1500.0</ind><dep>170.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>0</z-m>
        </center>
    </animation>

</PropertyList>