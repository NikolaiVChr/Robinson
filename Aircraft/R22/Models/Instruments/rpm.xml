<?xml version="1.0"?>

<PropertyList>

<path>rpm.ac</path>
<!-- The needles rotates 62 degrees from 50% to 100% or 1.24 degrees per percent -->
<!-- The needles sit 11.5 degrees below 50% so about 40% -->
<!-- Top of the scale is 96 degrees at 120% -->
    <animation>
        <type>rotate</type>
        <object-name>RPM.Eneedle</object-name>
        <property>engines/engine/n1</property>
        <interpolation>
            <entry><ind>0.0</ind><dep>0</dep></entry>
            <entry><ind>50</ind><dep>11.5</dep></entry>
            <entry><ind>100</ind><dep>73.5</dep></entry>
            <entry><ind>120</ind><dep>96.0</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>-0.027</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>RPM.Rneedle</object-name>
        <property>rotors/main/rpm</property>
        <interpolation>
            <entry><ind>204.0</ind> <dep>0.0 </dep></entry><!--  40% -->
            <entry><ind>255.0</ind><dep>11.5 </dep></entry><!--  50% -->
            <entry><ind>510.0</ind><dep>73.5 </dep></entry><!-- 100% -->
            <entry><ind>612.0</ind><dep>96.0 </dep></entry><!-- 120% -->
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0.027</y-m>
            <z-m>0.0</z-m>
        </center>
    </animation>

</PropertyList>