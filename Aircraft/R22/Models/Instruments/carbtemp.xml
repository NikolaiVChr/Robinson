<?xml version="1.0"?>

<PropertyList>

<path>carbtemp.ac</path>

    <animation>
        <type>rotate</type>
        <object-name>CARB.needle</object-name>
        <property>environment/temperature-degc</property>
        <interpolation>
            <entry><ind>-30.0</ind><dep>-47.0</dep></entry>
            <entry><ind>0.0</ind><dep>0.0</dep></entry>
            <entry><ind>30.0</ind><dep>47.0</dep></entry>
        </interpolation>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.006</z-m>
        </center>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>CARB.needle</object-name>
        <property>engines/engine/carb-temp-offset</property>
        <factor>1.566</factor>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
        <center>
            <x-m>0</x-m>
            <y-m>0</y-m>
            <z-m>-0.006</z-m>
        </center>
    </animation>

</PropertyList>